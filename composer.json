{
    "name": "awcodes/documental",
    "description": "This is my package documental",
    "keywords": [
        "awcodes",
        "laravel",
        "filamentphp",
        "documental"
    ],
    "homepage": "https://github.com/awcodes/documental",
    "support": {
        "issues": "https://github.com/awcodes/documental/issues",
        "source": "https://github.com/awcodes/documental"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Adam Weston",
            "email": "awcodes1@gmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "filament/filament": "^4.0",
        "phiki/phiki": "^2.0",
        "pomodocs/commonmark-alert": "^0.6.0",
        "ryangjchandler/commonmark-blade-block": "^1.0",
        "saade/filament-adjacency-list": "^4.0",
        "spatie/laravel-package-tools": "^1.15.0"
    },
    "require-dev": {
        "filament/upgrade": "^4.0",
        "larastan/larastan": "^3.0",
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^8.0",
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "files": [
            "src/helpers.php"
        ],
        "psr-4": {
            "Awcodes\\Documental\\": "src/",
            "Awcodes\\Documental\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Awcodes\\Documental\\Tests\\": "tests/src"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Awcodes\\Documental\\DocumentalServiceProvider"
            ],
            "aliases": {
                "Documental": "Awcodes\\Documental\\Facades\\Documental"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
